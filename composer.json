{
  "name": "itsnubix/shippy",
  "version": "0.0.1",
  "description": "A modern PHP library for generating shipping information",
  "keywords": [
    "shipping",
    "delivery",
    "postal"
  ],
  "support": {
    "issues": "https://github.com/itsnubix/shippy/issues",
    "source": "https://github.com/itsnubix/shippy"
  },
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "friendsofphp/php-cs-fixer": "^2"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kyle Milloy",
      "email": "kmilloy@nubix.ca"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Shippy\\": "src/Shippy/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
